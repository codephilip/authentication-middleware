{
  "name": "auth-middleware-example",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "nodemon --watch 'src/**/*.ts' --exec 'ts-node' src/server.ts",
    "build": "tsc",
    "start": "node dist/server.js",
    "swagger-gen": "typescript-to-swagger-gen -c src/swagger.ts -o swagger.json",
    "test": "jest --coverage",
    "test:watch": "jest --watch",
